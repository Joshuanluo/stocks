<h1>Sign in</h1>

<% if flash[:error].present? %>
    <h2><%= flash[:error] %></h2>
<% end %>

<%= form_with url: login_path do |f| %>
    <%= f.label :email %>
    <%= f.email_field :email %>
    <%= f.label :password %>
    <%= f.password_field :password %>

    <%= f.submit "Sign in"%>
<% end %>


